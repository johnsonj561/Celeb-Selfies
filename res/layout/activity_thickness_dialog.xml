<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/celebselfies_background1"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.putty.celebselfie_v2.ThicknessDialog" >

    <Button
    	    android:id="@+id/thickness2Button"
    	    android:background="@drawable/thickness_two_menu"
    	    android:layout_width="50dp"
    	    android:layout_height="50dp"
    	    android:layout_centerHorizontal="true"
    	    android:layout_alignParentTop="true"
    	    android:layout_marginTop="10dp"
    	    />

    	<Button
    	    android:id="@+id/thickness5Button"
    	    android:background="@drawable/thickness_five_menu"
    	    android:layout_width="50dp"
    	    android:layout_height="50dp"
    	    android:layout_below="@id/thickness2Button"
    	    android:layout_centerHorizontal="true"
    	    android:layout_marginTop="10dp"/>
    	
    	<Button
    	    android:id="@+id/thickness8Button"
    	    android:background="@drawable/thickness_eight_menu"
    	    android:layout_width="50dp"
    	    android:layout_height="50dp"
    	    android:layout_below="@id/thickness5Button"
    	    android:layout_marginTop="10dp"
    	    android:layout_centerHorizontal="true"/>
    
  	<Button
    	    android:id="@+id/thickness1Button"
    	    android:background="@drawable/thickness_one_menu"
    	    android:layout_width="50dp"
    	    android:layout_height="50dp"
    	    android:layout_alignParentTop="true"
    	    android:layout_marginTop="10dp"
    	    android:layout_toLeftOf="@id/thickness2Button"
    	    android:layout_toStartOf="@id/thickness2Button"
    	    android:layout_marginEnd="10dp"
    	    android:layout_marginRight="10dp"/>
    	    
    	<Button
    	    android:id="@+id/thickness4Button"
    	    android:background="@drawable/thickness_four_menu"
    	    android:layout_width="50dp"
    	    android:layout_height="50dp"
    	    android:layout_below="@id/thickness1Button"
    	    android:layout_marginTop="10dp"
    	    android:layout_toLeftOf="@id/thickness5Button"
    	    android:layout_toStartOf="@id/thickness5Button"
    	    android:layout_marginRight="10dp"
    	    android:layout_marginEnd="10dp"/>
    	
   		<Button
    	    android:id="@+id/thickness7Button"
    	    android:background="@drawable/thickness_seven_menu"
    	    android:layout_width="50dp"
    	    android:layout_height="50dp"
    	    android:layout_below="@id/thickness4Button"
    	    android:layout_marginTop="10dp"
    	    android:layout_toLeftOf="@id/thickness8Button"
    	    android:layout_toStartOf="@id/thickness8Button"
    	    android:layout_marginEnd="10dp"
    	    android:layout_marginRight="10dp"/>
    	
    	<Button
    	    android:id="@+id/thickness3Button"
    	    android:background="@drawable/thickness_three_menu"
    	    android:layout_width="50dp"
    	    android:layout_height="50dp"
    	    android:layout_alignParentTop="true"
    	    android:layout_marginTop="10dp"
    	    android:layout_toRightOf="@id/thickness2Button"
    	    android:layout_toEndOf="@id/thickness2Button"
    	    android:layout_marginStart="10dp"
    	    android:layout_marginLeft="10dp"/>
    	    
    	<Button
    	    android:id="@+id/thickness6Button"
    	    android:background="@drawable/thickness_six_menu"
    	    android:layout_width="50dp"
    	    android:layout_height="50dp"
    	    android:layout_below="@id/thickness3Button"
    	    android:layout_marginTop="10dp"
    	    android:layout_toRightOf="@id/thickness5Button"
    	    android:layout_toEndOf="@id/thickness5Button"
    	    android:layout_marginLeft="10dp"
    	    android:layout_marginStart="10dp"/>
    	
   		<Button
    	    android:id="@+id/thickness9Button"
    	    android:background="@drawable/thickness_nine_menu"
    	    android:layout_width="50dp"
    	    android:layout_height="50dp"
    	    android:layout_below="@id/thickness6Button"
    	    android:layout_marginTop="10dp"
    	    android:layout_toRightOf="@id/thickness8Button"
    	    android:layout_toEndOf="@id/thickness8Button"
    	    android:layout_marginStart="10dp"
    	    android:layout_marginLeft="10dp"/>


</RelativeLayout>